#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.8.5 at
# Tue, 12 Apr 2016 02:12:11 -0400

%:
	dh $@ --with python2 --buildsystem=python_distutils


override_dh_auto_clean:
	python setup.py clean -a
	find . -name \*.pyc -exec rm {} \;



override_dh_auto_build:
	python setup.py build --force



override_dh_auto_install:
	python setup.py install --force --root=debian/python-ncclient --no-compile -O0 --install-layout=deb  --install-scripts=/trash
	rm -rf debian/python-ncclient/trash
	rm -rf debian/python3-ncclient/trash


override_dh_python2:
	dh_python2 --no-guessing-versions




